[Plugins]
Output=i3bar
Input=mpd;pulseaudio;nm;upower;time

[CPULoad]
Frequency=2

[Override cpu]
Label=cpu

[Memory]
Frequency=5

[Override mem]
Label=mem

[Filesystem]
Names=home

[Filesystem home]
Format=${p_used}%
Label=home
Device=/dev/mapper/MyStorage-homevol

[Time]
Zones=America/Los_Angeles;Europe/London
Formats=%a %d %b %T;%T %Z

[NetworkManager]
Interfaces=enp0s25;wlp3s0
HideUnavailable=true

[NetworkManager Formats]
Addresses=IPv4
UpWiFi=${addresses}${ (<ssid>)}

[Sensors]
Sensors=coretemp-isa-0000

